{
  "name": "rx-component",
  "version": "0.0.8",
  "description": "Functional, reactive React components",
  "main": "rx-component.js",
  "types": "rx-component.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "examples": "budo examples/index.tsx -d assets -o -v --index assets/index.html -- --debug -p tsify",
    "build-examples": "browserify examples/index.tsx --debug -p tsify -o assets/index.js",
    "lint": "tslint -c tslint.json -p tsconfig.json '*.tsx'"
  },
  "author": "Felix Becker <felix.b@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/felixfbecker/rx-component"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.0.7",
    "@types/react-dom": "^16.0.2",
    "browserify": "^14.4.0",
    "budo": "^10.0.4",
    "react-dom": "^16.0.0",
    "rxjs-tslint-rules": "^3.0.3",
    "tsify": "^3.0.3",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "react": ">=15.6.1 <17.0.0",
    "rxjs": "^5.4.3"
  }
}
